<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="RefreshFrameLayout">
    <!-- the resistance when you are moving the frame -->
    <attr format="float" name="resistance"/>
    <!-- the ration of the height of the header to trigger refresh -->
    <attr format="float" name="ratio_of_header_height_to_refresh"/>
    <attr format="integer" name="duration_to_close"/>
    <attr format="integer" name="duration_to_close_header"/>
    <attr format="integer" name="duration_to_close_footer"/>
    <!-- pull to refresh, otherwise release to refresh, default is release to refresh -->
    <attr format="boolean" name="enable_refresh"/>
    <attr format="boolean" name="enable_load"/>
    <!-- keep header when refreshing-->
    <attr format="boolean" name="keep_header_when_refresh"/>
    <attr format="boolean" name="keep_footer_when_refresh"/>
  </declare-styleable>
</resources>